{
  "name": "expresstomatch",
  "version": "1.0.0",
  "description": "Express To Match",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development npm run build-css && pm2 start server/server.js &",
    "copy-assets": "rm -rf server/public/images && cp -r server/images server/public/images &",
    "build-css": "rm -rf server/public/css && node_modules/node-sass/bin/node-sass --include-path server/scss server/scss/main.scss server/public/css/main.css --output-style compressed &",
    "debug:dev": "NODE_ENV=development npm run build-css && node --inspect server/server.js &",
    "prod:build": "npm run build-css && npm run copy-assets & node_modules/webpack/bin/webpack.js --config webpack.config.prod.js --mode production &",
    "prod": "NODE_ENV=production npm run prod:build && pm2 start server/prodServer.js &",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ExpressToMatch",
  "license": "UNLICENSED",
  "private": true,
